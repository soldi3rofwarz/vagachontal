{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }], 
      "redirects":[{
        "source":"/login",
        "destination": "/login",
        "type":301
      }]
  }
}
